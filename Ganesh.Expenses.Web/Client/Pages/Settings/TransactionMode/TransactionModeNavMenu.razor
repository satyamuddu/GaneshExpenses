<MudList Clickable=true>
    <MudListItem Text="TransactionModes" Icon="@Icons.Material.Outlined.Extension" Dense="true" Class="@GetClass("transactionModeslist")"/>
    <MudListItem Href="pages/settings/addtransactionmode" Text="Add TransactionMode" Icon="@Icons.Material.Outlined.Add" Dense="true" Class="@GetClass("addtransactionmode")"/>
</MudList>

@code {
    [Parameter] public string folder { get; set; }

    public string GetClass(string _folder)
    {
        if (folder == _folder)
        {
            return "my-2 rounded mud-primary-text";
        }
        else
        {
            return "my-2 rounded";
        }
    }
}
